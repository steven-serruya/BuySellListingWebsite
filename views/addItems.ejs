<!doctype html>
<html lang="en">

<% include head %>


  <body>
    <!-- NAV BAR-->

    <% include navbarLI %>


      <!-- Carousel-->


      <!--Main COntainer-->
      <form method="post" action="/sell">
        <div class="form-group col-md-6 offset-md-1">
          <label for="itemName">Item name</label>
          <input type="text" class="form-control" id="itemName" name="itemName" placeholder="Item name" required>
        </div>
        <div class="form-group col-md-6 offset-md-1">
          <label for="price">Price</label>
          <input type="text" class="form-control" id="price" name="price" placeholder="Price" required>
        </div>
        <div class="form-group col-md-6 offset-md-1">
          <label for="description">Description</label>
          <input type="text" class="form-control" id="description" name="description" placeholder="Description of item"
            required>
        </div>
        <div class="form-group col-md-6 offset-md-1">
          <label for="detailed_description">More detailed description</label>
          <input type="text" class="form-control" id="detailed_description" name="detailed_description"
            placeholder="A more detailed description of item" required>
        </div>
        <div class="form-group col-md-6 offset-md-1">
          <label for="picUrl">Pic Url</label>
          <input type="text" class="form-control" id="picUrl" name="picUrl" placeholder="Add the URL of your picture"
            required>
        </div>
        <div class="form-group">
          <div class="form-check"></div>
        </div>
        <button type="submit" class="btn btn-primary col-md-6 offset-md-1">Sell Item</button>
      </form>


      <!-- Include your JavaScript links here -->
      <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/js/bootstrap.bundle.min.js"
        integrity="sha384-HwwvtgBNo3bZJJLYd8oVXjrBZt8cqVSpeBNS5n7C8IVInixGAoxmnlMuBnhbgrkm"
        crossorigin="anonymous"></script>
  </body>

</html>